@model Student

@{
    
    ViewBag.Title = "ManageStudent  | Add";
}

<p class="alert alert-info my-4 text-center"> Add New Student </p>
<form  class="mx-lg-auto w-50" action="AddStudent" controller="Student" method="post">
    <div class="form-group">
        <label for="cin">CIN</label>
        <input type="text" class="form-control" id="cin" value="">
    </div>
    <div class="form-group">
        <label for="prenom">Prenom</label>
        <input type="text" class="form-control" id="prenom" value="">
    </div>
    <div class="form-group">
        <label for="nom">Nom</label>
        <input type="text" class="form-control" id="nom" value="">
    </div>
    <div class="form-group">
        <label for="adresse">Adresse</label>
        <input type="text" class="form-control" id="adresse" value="">
    </div>
    <label for="filiere">Filiére</label>
    <select class="form-control custom-select" id="filiere" value="">
        <option>4</option>
        <option>5</option>


    </select>

    <div class="form-check">
        <input type="checkbox" class="form-check-input" id="active" checked value="1">
        <label class="form-check-label" for="active">IsActive</label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>